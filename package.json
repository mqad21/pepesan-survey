{
  "name": "pepesan-survey",
  "version": "1.0.0",
  "description": "An extension of Pepesan to create interactive survey in WhatsApp chat bot",
  "main": "index.js",
  "scripts": {
    "prepare": "tsc",
    "watch": "tsc --watch",
    "example": "tsc && nodemon ./example/index.js",
    "test": "tsc && jest",
    "watch-test": "tsc && jest --watch-all",
    "dev": "nodemon ./src/index.ts",
    "debug": "nodemon --exec \"node --inspect-brk=0.0.0.0:9229 --require ts-node/register ./example/index.js\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mqad21/pepesan-survey.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/mqad21/pepesan-survey/issues"
  },
  "homepage": "https://github.com/mqad21/pepesan-survey#readme",
  "devDependencies": {
    "@jest/globals": "^29.2.2",
    "@types/express": "^4.17.17",
    "@types/qrcode": "^1.5.2",
    "@types/ws": "^8.5.3",
    "jest": "^29.2.2",
    "jest-circus": "^29.3.1",
    "nodemon": "^3.0.1",
    "ts-jest": "^29.0.3",
    "ts-node": "^10.9.1",
    "ts-node-dev": "^2.0.0",
    "typescript": "^4.8.4"
  },
  "dependencies": {
    "pepesan": "^3.4.0"
  }
